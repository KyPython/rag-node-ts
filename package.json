{
  "name": "rag-node-ts",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "ingest": "ts-node-dev src/scripts/ingestLocal.ts",
    "setup-pinecone": "ts-node-dev src/scripts/setupPinecone.ts",
    "bench": "ts-node-dev src/benchmark/benchmark.ts",
    "bench:all": "npm run build && node dist/benchmark/benchmark.js --url http://localhost:3000 --requests 20 --concurrency 3 --mode mock",
    "bench:load": "npm run build && node dist/benchmark/benchmark.js --url http://localhost:3000 --requests 50 --concurrency 10 --cacheMode semantic --mode mock",
    "bench:real": "npm run build && ALLOW_REAL_BENCH=1 node dist/benchmark/benchmark.js --url http://localhost:3000 --requests 20 --confirm-real",
    "judge": "ts-node-dev src/benchmark/judge.ts",
    "demo": "npm run dev",
    "compose:up": "docker-compose up -d",
    "demo-setup": "bash scripts/demo-setup.sh",
    "test": "npm run build && node --experimental-vm-modules ./node_modules/.bin/jest --runInBand",
    "test:fast": "node --experimental-vm-modules ./node_modules/.bin/jest --runInBand",
    "lint": "eslint --ext .ts .",
    "ci": "npm run lint && npm run build && npm test",
    "prepare": "husky install",
    "release": "semantic-release",
    "docker:build": "docker build -t rag-node-ts:latest ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@langchain/community": "^1.0.7",
    "@langchain/core": "^1.1.4",
    "@langchain/openai": "^1.1.3",
    "@pinecone-database/pinecone": "^6.1.3",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "ioredis": "^5.8.2",
    "langchain": "^1.1.5",
    "morgan": "^1.10.1",
    "multer": "^2.0.0",
    "openai": "^6.10.0",
    "pdf-parse": "^2.4.5",
    "pino": "^8.21.0",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/ioredis": "^4.28.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.10.1",
    "@types/pdf-parse": "^1.1.5",
    "pino-pretty": "^13.1.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  ,
    "husky": "^8.0.0",
    "lint-staged": "^14.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.3",
    "semantic-release": "^22.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/commit-analyzer": "^10.0.0",
    "@semantic-release/release-notes-generator": "^10.0.0"
  }
  
}
